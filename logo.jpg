<!-- LOGO: esfera de linhas (inline SVG) -->
<svg class="logo-ms" width="52" height="52" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg" aria-label="Mentaliizi Studios logo">
  <!-- fundo -->
  <rect width="512" height="512" fill="#000"/>
  <defs>
    <!-- recorte circular (a “borda” da esfera) -->
    <clipPath id="clipSphere">
      <circle cx="256" cy="256" r="240"/>
    </clipPath>
    <!-- leve sombreamento radial para dar volume -->
    <radialGradient id="shade" cx="50%" cy="50%" r="55%">
      <stop offset="0%" stop-color="#000" stop-opacity="0"/>
      <stop offset="80%" stop-color="#000" stop-opacity="0.35"/>
      <stop offset="100%" stop-color="#000" stop-opacity="0.55"/>
    </radialGradient>
  </defs>

  <!-- linhas horizontais, cortadas pelo círculo -->
  <g clip-path="url(#clipSphere)" stroke="#e9e9e9" stroke-linecap="round">
